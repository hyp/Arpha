# temporary module used to test how functions resolve with parameters depending on each other

def foo0(x) typeof(x) = x
foo0(1) # ok

def foo(x,v typeof(x)) = 1
foo(1,2) # ok
foo(1,false) # not resolved!

def foo1(x,v typeof(x)) = x,v
foo1(true,false) # ok

